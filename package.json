{
  "name": "rabbit-queue",
  "version": "5.2.0",
  "description": "AMQP/RabbitMQ queue management library.",
  "main": "js/index.js",
  "keywords": [
    "rabbitmq",
    "rabbit",
    "queue",
    "amqp",
    "amqplib"
  ],
  "scripts": {
    "build": "tsc -p ts",
    "watch": "tsc -w -p ts",
    "prepublishOnly": "npm run build && npm run test-docker",
    "pretest": "tslint --project ./ts/ && tslint  ./test/**/*.ts && npm run build",
    "test": "nyc mocha",
    "test-docker": "docker-compose run rabbit-queue npm run test",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --print-width 120 --single-quote true --write",
      "git add"
    ]
  },
  "nyc": {
    "branches": 70,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "include": [
      "ts/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "check-coverage": true,
    "sourceMap": true,
    "instrument": true
  },
  "repository": "Workable/rabbit-queue",
  "author": "Nikos Kostoulas <kostoulas@workable.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "amqplib": "0.8.0",
    "race-until": "~2.3.1",
    "uuid": "~8.3.2"
  },
  "typings": "js/index",
  "devDependencies": {
    "@types/amqplib": "0.8.1",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.4.1",
    "@types/should": "^13.0.0",
    "@types/sinon": "~10.0.2",
    "@types/source-map-support": "~0.5.4",
    "@types/uuid": "~8.3.1",
    "husky": "~7.0.1",
    "lint-staged": "~11.1.0",
    "mocha": "~9.0.2",
    "nyc": "~15.1.0",
    "should": "^13.2.3",
    "sinon": "~11.1.1",
    "source-map-support": "~0.5.19",
    "ts-node": "~10.1.0",
    "tslint": "^6.0.0",
    "typescript": "4.3.5"
  }
}
