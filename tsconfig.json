{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./js",
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": ["es6", "esnext.asynciterable"]
  },
  "filesGlob": ["./*.ts"]
}
